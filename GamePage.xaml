<Page x:Class="WindowsSnake.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WindowsSnake"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="GamePage"
      KeyDown="ProcessKeyStrokes"
      Focusable="True"
      FocusVisualStyle="{x:Null}">
  <Border BorderBrush="Black" BorderThickness="5" Focusable="False" x:Name="GameBorder">
    <Grid x:Name="GameGrid" Focusable="False">
    
    </Grid>
  </Border>
</Page>
